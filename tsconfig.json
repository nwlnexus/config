{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./src/typescript.json",
  "include": ["**/*.ts", "**/*.js"],
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "emitDeclarationOnly": true,
    "noEmit": false,
    "outDir": "./dist",
    "tsBuildInfoFile": "node_modules/.cache/tsbuildinfo.json",
    "types": ["./reset.d.ts"]
  }
}
