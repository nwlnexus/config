{
  "$schema": "https://json.schemastore.org/package",
  "name": "@nwlnexus/config",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.0.0-semantically-released",
  "main": "src/index.js",
  "type": "module",
  "keywords": [
    "config",
    "nwlnexus",
    "prettier",
    "typescript",
    "eslint"
  ],
  "author": {
    "name": "Nigel Williams-Lucas",
    "email": "info@nwlnexus.com",
    "url": "https://github.com/nwlnexus"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nwlnexus/config.git"
  },
  "bugs": {
    "url": "https://github.com/nwlnexus/config/issues"
  },
  "homepage": "https://github.com/nwlnexus/config#readme",
  "readme": "https://github.com/nwlnexus/config#readme",
  "license": "MIT",
  "exports": {
    ".": "./src/index.js",
    "./eslint": {
      "import": "./src/eslint.js",
      "types": "./dist/src/eslint.d.ts"
    },
    "./typescript": "./src/library.json",
    "./react-library": "./src/react-library.json",
    "./reset.d.ts": "./reset.d.ts",
    "./remix": "./src/remix.json",
    "./worker": "./src/worker.json",
    "./prettier": "./src/prettiercfg.js"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf ./dist",
    "lint": "eslint .",
    "format": "prettier . --write",
    "typecheck": "tsc",
    "validate": "run-p -l format lint typecheck"
  },
  "dependencies": {
    "@eslint/compat": "^1.2.0",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.12.0",
    "@remix-run/eslint-config": "^2.12.1",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/eslint": "^9.6.1",
    "@types/eslint-config-prettier": "^6.11.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-import-x": "^4.3.1",
    "eslint-plugin-jest-dom": "^5.4.0",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-svelte": "^2.44.1",
    "eslint-plugin-testing-library": "^6.3.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-vitest": "^0.5.4",
    "globals": "^15.11.0",
    "import-meta-resolve": "^4.1.0",
    "prettier-plugin-svelte": "^3.2.7",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "typescript-eslint": "^8.8.1"
  },
  "devDependencies": {
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.11",
    "eslint": "^9.12.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "rimraf": "^6.0.1",
    "typescript": "^5.6.0"
  },
  "overrides": {
    "eslint": "$eslint",
    "@typescript-eslint/eslint-plugin": ">= 8.8.1",
    "@typescript-eslint/parser": ">= 8.8.1",
    "@typescript-eslint/utils": ">= 8.8.1"
  },
  "peerDependencies": {
    "eslint": ">= 9.10.0",
    "prettier": ">=3.3.3",
    "typescript": ">= 5.6.0"
  },
  "prettier": "./src/prettiercfg.js",
  "packageManager": "pnpm@9.9.0"
}
